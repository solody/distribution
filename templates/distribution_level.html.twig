{#
/**
 * @file distribution_level.html.twig
 * Default theme implementation to present Level data.
 *
 * This template is used when viewing Level pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_distribution_level()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('distribution_level') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
